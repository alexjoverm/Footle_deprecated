{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "jsx": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": 0,
        "indent": [2, 2, {"SwitchCase": 1}],
        "no-multi-spaces": [1, {
            "exceptions": {
                "VariableDeclarator": true
            }
        }],
        "react/prop-types": 0
    }
}
